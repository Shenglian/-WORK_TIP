console.log('1', Date.parse(compareTime) > Date.parse(currentTime));


function removeRepeatValue(array) {
  // set object
  let setObject = new Set(array);
  // array object
  let arrayObject = Array.from(setObject);
  return arrayObject;
}

console.log('result', removeRepeatValue([1,2,,4,5,7,8,79,,678,678,78,678,1000]));


