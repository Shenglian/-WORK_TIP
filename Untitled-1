newFunction();

function newFunction() {
  console.log('0', Math.floor(3.45));
  console.log('1', 1.23 | 0);
  console.log('1-1', 1.56 | 0);
  console.log('2', 2 | 0);
  console.log('3', - 4 | 0);
}

