function eq(type) {
  var string = Object.prototype.toString;
  return string.call(type);
}

console.log(eq('123'));
console.log(eq({1: '123'}));
console.log(eq([1,2,3,4,5,6,7,100]));

console.log(eq(null));
console.log(eq(undefined));

console.log(eq(1));
console.log(eq(-1));
console.log(eq(+1));

console.log(eq(0.1));
console.log(eq(-0.1));
console.log(eq(+0.1));

console.log(eq(0));
console.log(eq(-0));
console.log(eq(+0));


var person = {
  name: 'sheng',
  age: 20,
};

function ad() {
  console.log.call(null, this.name, this.age);
}

Function.prototype.callme = function(content) {
  content.fn = this;
  content.fn();
  delete content.fn;
}

ad.callme(person);

