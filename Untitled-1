"use strict"

function f(a) {
  var args = Array.prototype.slice.call(arguments, f.length);
  return sum(a, args);
}

function sum(a, bs) {
  return bs.map(function(b, i){
    return b * a;
  });
}

console.log('f => ', f(2,3,4,4,5,6,7,8,9,0,111));


function what({type, id} = {}, {type2, id2} = {}) {
  return `${type} - ${id}`;
}

|||

function what() {
  var _ref = arguments.length > 0 && arguments[0] !== undefined ? arguments[0] : {},
      type = _ref.type,
      id = _ref.id;

  var _ref2 = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : {},
      type2 = _ref2.type2,
      id2 = _ref2.id2;

  return type + " - " + id;
}


